            h5.text-white.mb-3= lang_versions['col_name_2']
            ul.list-unstyled.mb-0
              li.mb-1
                a(href=`${CWD}pages/404.html`).text-700.hover-color-white= lang_versions['ai_assistants']
              li.mb-1
                a(href=`${CWD}pages/404.html`).text-700.hover-color-white= lang_versions['semantic_knowledge_bases']
