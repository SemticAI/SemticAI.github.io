        //-     a(href="#!").text-900.hover-color-white.px-2
        //-       span.fab.fa-twitter
        //-   li.list-inline-item.me-0
        //-     a(href="#!").text-900.hover-color-white.px-2
        //-       span.fab.fa-linkedin-in
        //-   li.list-inline-item.me-0
        //-     a(href="#!").text-900.hover-color-white.px-2
        //-       span.fab.fa-google-plus-g
      .col-lg-6
        .row
          .col-6.col-md-5.ps-lg-4.mb-4.mb-lg-0
            h5.text-white.mb-3= lang_versions['col_name_1']
            ul.list-unstyled.mb-0
              li.mb-1
                a(href=`#services`).text-700.hover-color-white= lang_versions['services']
              li.mb-1
                a(href=`#whyus`).text-700.hover-color-white= lang_versions['whyus']
